{
  "name": "rescript-snippets",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "test_retest": "NODE_OPTIONS=--experimental-vm-modules retest __tests__/*.mjs",
    "test99": "npx nodemon --exec NODE_OPTIONS=--experimental-vm-modules retest src/Problems_in_OCaml/*_Test.mjs"
  },
  "fig": {
    "test": {
      "description": "Run all tests",
      "icon": "üß™",
      "priority": 100
    },
    "build": {
      "description": "Build the project",
      "icon": "üõ†"
    },
    "start": {
      "description": "Start watching and building project",
      "icon": "‚≠êÔ∏è"
    },
    "clean": {
      "description": "Clean the project",
      "icon": "üõÄ"
    }
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|mjs|jsx|ts|tsx)",
      "<rootDir>/**/__tests__/*_Test.mjs"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@rescript/core|@glennsl/rescript-jest|@ryyppy/rescript-promise|rationale)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "devDependencies": {
    "@glennsl/rescript-jest": "https://github.com/dsiu/rescript-jest#rescript-v12",
    "@swc/core": "^1.10.4",
    "@swc/jest": "^0.2.37",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.7.0",
    "rescript": "^12.0.0-alpha.7",
    "rescript-test": "git+https://github.com/dsiu/rescript-test#rescript-v12"
  },
  "dependencies": {
    "@dsiu/rescript-stdlib-fp": "git+https://github.com/dsiu/rescript-stdlib-fp#rescript-v12",
    "@jmagaram/rescript-extras": "git+https://github.com/dsiu/rescript-extras#rescript-v12",
    "@jmagaram/rescript-seq": "git+https://github.com/dsiu/rescript-seq#rescript-v12",
    "@mobily/rescript-date": "^2.1.0",
    "add": "^2.0.6",
    "bs-fetch": "^0.6.2",
    "d3": "^7.9.0",
    "isomorphic-fetch": "^3.0.0",
    "js-array2-ex": "git+https://github.com/dsiu/rescript-js-array2-extension#rescript-v12",
    "ms": "^2.1.3",
    "nodemon": "^3.1.9",
    "rationale": "^0.2.0",
    "rescript-bag": "git+https://github.com/dsiu/rescript-bag#rescript-v12",
    "rescript-nodejs": "^16.1.0"
  }
}
