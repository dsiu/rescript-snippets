{
  "name": "rescript-snippets",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "clean": "rescript clean -with-deps",
    "start": "rescript build -w",
    "test": "jest",
    "test_retest": "NODE_OPTIONS=--experimental-vm-modules retest __tests__/*.mjs",
    "test99": "npx nodemon --exec NODE_OPTIONS=--experimental-vm-modules retest src/Problems_in_OCaml/*_Test.mjs"
  },
  "fig": {
    "test": {
      "description": "Run all tests",
      "icon": "üß™",
      "priority": 100
    },
    "build": {
      "description": "Build the project",
      "icon": "üõ†"
    },
    "start": {
      "description": "Start watching and building project",
      "icon": "‚≠êÔ∏è"
    },
    "clean": {
      "description": "Clean the project",
      "icon": "üõÄ"
    }
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|mjs|jsx|ts|tsx)",
      "<rootDir>/**/__tests__/*_Test.mjs"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "@swc/jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@glennsl/rescript-jest|@ryyppy/rescript-promise|rationale)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "devDependencies": {
    "@glennsl/bs-jest": "*",
    "@glennsl/rescript-jest": "^0.9.1",
    "@swc/core": "^1.3.10",
    "@swc/jest": "^0.2.22",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.2.1",
    "rescript": "^10.0.0",
    "rescript-test": "^4.0.0"
  },
  "dependencies": {
    "@glennsl/bs-json": "^5.0.4",
    "@mobily/rescript-date": "^2.1.0",
    "@ryyppy/rescript-promise": "latest",
    "add": "^2.0.6",
    "bs-bastet": "github:dsiu/bastet#rescript-v10",
    "bs-fetch": "^0.6.2",
    "d3": "^7.6.1",
    "isomorphic-fetch": "^3.0.0",
    "ms": "^2.1.3",
    "js-array2-ex": "^0.0.4",
    "nodemon": "^2.0.19",
    "relude": "github:dsiu/relude#rescript-v10",
    "rescript-bag": "github:dsiu/rescript-bag#main",
    "rescript-js": "^1.0.0-beta.2",
    "rescript-jzon": "latest",
    "rescript-nodejs": "^14.3.1",
    "rescript-ocamlgraph": "github:dsiu/rescript-ocamlgraph#rescript-v10",
    "tablecloth-rescript": "github:darklang/tablecloth-rescript#main",
    "@dsiu/rescript-stdlib-fp": "github:dsiu/rescript-stdlib-fp"
  }
}
